import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int i = 0; i < t; i++){
		    long a = sc.nextLong();
		    long b = sc.nextLong();
		    long c = sc.nextLong();
		    long d = sc.nextLong();
		    if(a < b){
		        boolean cnt = false;
		        if(c < d){
		            while(a < b){
		                a += d;
		                b += c;
		                if(a == b){
		                    cnt = true;
		                    break;
		                }
		            }
		            if(cnt){
		                System.out.println("YES");
		            }else{
		                System.out.println("NO");
		            }
		        }else if(c > d){
		            while(a < b){
		                a += c;
		                b += d;
		                if(a == b){
		                    cnt = true;
		                    break;
		                }
		            }
		            if(cnt){
		                System.out.println("YES");
		            }else{
		                System.out.println("NO");
		            }
		        }else{
		            System.out.println("NO");
		        }
		    }else if(a > b){
		        boolean cnt = false;
		        if(c < d){
		            while(a > b){
		                a += c;
		                b += d;
		                if(a == b){
		                    cnt = true;
		                    break;
		                }
		            }
		            if(cnt){
		                System.out.println("YES");
		            }else{
		                System.out.println("NO");
		            }
		        }else if(c > d){
		            while(a < b){
		                a += d;
		                b += c;
		                if(a == b){
		                    cnt = true;
		                    break;
		                }
		            }
		            if(cnt){
		                System.out.println("YES");
		            }else{
		                System.out.println("NO");
		            }
		        }else{
		            System.out.println("NO");
		        }
		    }else{
		        if(c != d){
		            System.out.println("NO");
		        }else{
		            System.out.println("YES");
		        }
		    }
		}
	}
}


